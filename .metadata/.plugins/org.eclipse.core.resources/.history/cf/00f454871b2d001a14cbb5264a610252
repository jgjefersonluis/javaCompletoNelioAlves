
public class Main {

	public static void main(String[] args) {
		
		int y = 48;
		double x = 10.35784;
		
		System.out.print("Ola, mundo!");
		System.out.println("Bom dia!");
		System.out.println(y);
		System.out.println(x);
		System.out.printf("%.2f%n", x);
		System.out.printf("%.4f%n", x);

	}

}
